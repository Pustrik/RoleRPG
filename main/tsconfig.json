{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "es6",
      "dom"
    ],
    "module": "node16",
    "moduleResolution": "node",
    "declaration": false,
    "sourceMap": true,
    "inlineSources": true,
    "types": [
      "node"
    ],
    "stripInternal": true,
    "incremental": true,
    "skipLibCheck": true,
    "importsNotUsedAsValues": "error",
    "inlineSourceMap": false,
    "noEmit": false
  },
  "files": [
    "index.ts",
    "entityes/warrior.ts",
    "entityes/mage.ts",
    "entityes/thief.ts",
    "entityes/character.ts",
    "entityes/create_character.ts",
    "app.ts",
    "routes/auth.ts"
  ],

}
